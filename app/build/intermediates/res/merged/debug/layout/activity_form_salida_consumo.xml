<?xml version="1.0" encoding="utf-8"?>

<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    <TextView
        android:id="@+id/textViewCocepto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Concepto" />
     
	<AutoCompleteTextView
        android:id="@+id/editTextConcepto"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:inputType="textMultiLine" >        
    </AutoCompleteTextView>
    <TextView
        android:id="@+id/textViewReferencia"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Referencia" />
     
	<EditText
        android:id="@+id/editTextSReferencia"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textMultiLine" >
    </EditText>

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Selecione un almacen" />

    <Spinner
        android:id="@+id/spinner_salida_insummo_almacenes"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <ListView
        android:id="@+id/listViewMaterialesAlmacen"
        android:layout_width="match_parent"
        android:layout_height="120dp" >
    </ListView>

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Lista de materiales de salida" />

    <ListView
        android:id="@+id/listViewSalidaInsumos"
        android:layout_width="match_parent"
        android:layout_height="120dp" >
    </ListView>
    

     
    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Observaciones" />
    <EditText
        android:id="@+id/editTextSObservaciones"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textMultiLine" >

      
    </EditText>

    <Button
        android:id="@+id/buttonSalidaInsumos"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="guardarsalidainsumo"
        android:text="Guardar" />

	</LinearLayout>

